{
  "name": "crm-strom",
  "version": "1.0.0",
  "description": "Node.js app with Socket.io, TypeScript and Tailwind CSS",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "build:css": "tailwindcss -i ./src/input.css -o ./public/styles.css",
    "dev": "concurrently \"tsc -w\" \"tailwindcss -i ./src/input.css -o ./public/styles.css --watch\" \"nodemon dist/server.js\"",
    "start": "npm run build && npm run build:css && node dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.937.0",
    "@supabase/supabase-js": "^2.81.1",
    "@types/archiver": "^7.0.0",
    "@types/ejs": "^3.1.5",
    "archiver": "^7.0.1",
    "axios": "^1.6.2",
    "axios-cookiejar-support": "^5.0.2",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^17.2.3",
    "dropbox": "^10.34.0",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "pdfkit": "^0.13.0",
    "socket.io": "^4.6.1",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "@types/pdfkit": "^0.12.12",
    "@types/tough-cookie": "^4.0.5",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  }
}
